<table border = "1" style="text-align:center">
    <tr>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Address</th>
        <th>Email</th>
        <th>Telephone</th>
        <th>Registration Date</th>
    </tr>
    {% for cust in customers %}
        <tr>
            <td>{{ cust.firstname }}</td>
            <td>{{ cust.lastname }}</td>
            <td>{{ cust.address }}</td>
            <td>{{ cust.email }}</td>
            <td>{{ cust.telephone }}</td>
            <td>{{ cust.registrationDate|format_datetime()}}</td>
        </tr>
    {% endfor %}
</table>